<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="from_to_text">
        <item name="android:textSize">@dimen/frag_text_size_to_from</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="frag_switch">
        <item name="android:textSize">@dimen/switch_text_size</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="login_edittext">
        <item name="android:gravity">center</item>
        <item name="android:padding">8dp</item>
        <item name="android:textColor">@color/whites</item>
        <item name="android:background">@drawable/edi_text_rounded</item>
    </style>

    <style name="register_tel">
        <item name="android:paddingTop">4dp</item>
        <item name="android:layout_marginLeft">@dimen/register_margin_left</item>
        <item name="android:layout_marginRight">@dimen/register_margin_right</item>
        <item name="android:layout_marginTop">@dimen/register_margin_top</item>
        <item name="android:layout_height">@dimen/regsiter_box_height</item>
    </style>
</resources>